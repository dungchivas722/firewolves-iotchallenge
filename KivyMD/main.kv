
#: import md_icons kivymd.icon_definitions.md_icons
<ItemDrawer>:
    theme_text_color: "Custom"
    on_release: self.parent.set_color_item(self)

    IconLeftWidget:
        id: icon
        source: "log/uet.png"
        theme_text_color: "Custoim"
