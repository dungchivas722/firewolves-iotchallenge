        adaptive_height: True
    MDLabel:
        text: "+Phạm Đăng Du"
        font_style: "Caption"
        adaptive_height: True
    MDLabel:
        text: "+Nguyễn Kiên"
        font_style: "Caption"
        adaptive_height: True
