<BottomCard@MDCard>:
    radius:[25]
    image:""
    size_hint_x:None
    spacing:dp(10)
    padding:dp(10)
    elevation:15
    FitImage:
        source: root.image
        radius: [20,]
<TabLabel@Label>:
    color:0,0,0,0
    bg_color:0,0,0,0
    canvas.before:
        Color:
            rgba:self.bg_color
        RoundedRectangle:
            radius:[25]
            size:self.size[0]+10,self.size[1]
            pos:self.pos[0]-5,self.pos[1]

    size_hint_y:None
    size_hint_x:None
    width:self.texture_size[0]
    height:dp(50)
    color:0,0,0,1

<MagicButton@MagicBehavior+MDIconButton>


<MySwiper@MDSwiperItem>
    image:''
    RelativeLayout:
        FitImage:
            source: root.image
            radius: [20,]
        MDBoxLayout:
            orientation:'vertical'
            pos_hint:{'x':(root.width-self.width-dp(140))/root.width,"y":.8}
            adaptive_size:True
            padding:4
            MagicButton:
                id: icon
                icon: "camera-wireless"
                opacity: 0
                user_font_size: "50sp"
                theme_text_color:'Custom'
                text_color:0,1,0,1
                adaptive_height:True


        MDCard:
            size_hint_y:None
            size_hint_x:None
            width:root.width-dp(700)
            height:dp(65)
            pos_hint:{'center_x':.5,"y":.05}
            spacing: "12dp"
            md_bg_color:.9,.9,.9,.2
            orientation:"vertical"
            padding:dp(10)
